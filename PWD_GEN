COMMAND="$1"

function PWDGEN_CLI()
{

}

function PWDGEN_GUI()
{

}

if [[ ("${COMMAND}" = "CLI") ]] || \
[[ ("${COMMAND}" = "cli") ]] || \
[[ ("${COMMAND}" = "-C") ]] || \
[[ ("${COMMAND}" = "-c") ]]
    then
        PWDGEN_CLI
fi

if [[ ("${COMMAND}" = "GUI") ]] || \
[[ ("${COMMAND}" = "gui") ]] || \
[[ ("${COMMAND}" = "-G") ]] || \
[[ ("${COMMAND}" = "-g") ]]
    then
        PWDGEN_GUI
fi